{
  "name": "generator-latex-template",
  "version": "0.7.0",
  "description": "Generates a latex template to be used as start for writing scientific works (papers, articles, theses)",
  "homepage": "https://github.com/latextemplates/generator-latex-template",
  "author": {
    "name": "Oliver Kopp",
    "email": "kopp.dev@gmail.com",
    "url": "http://orcid.org/0000-0001-6962-4290"
  },
  "files": [
    "generators"
  ],
  "main": "generators/app/index.js",
  "keywords": [
    "latex",
    "bachelor thesis",
    "master thesis",
    "template",
    "yeoman-generator"
  ],
  "devDependencies": {
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-xo": "^0.41.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^28.1.3",
    "lint-staged": "^13.2.3",
    "prettier": "^2.8.8"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "basic-ftp": "^5.0.3",
    "chalk": "~4.1.2",
    "minimist": "^1.2.8",
    "unzipper": "^0.10.14",
    "yeoman-generator": "~5.9.0",
    "yeoman-option-or-prompt": "^2.0.1",
    "yosay": "^2.0.2"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "pretest": "eslint .",
    "test": "jest"
  },
  "repository": "https://github.com/latextemplates/generator-latex-template.git",
  "license": "0BSD"
}
