{
  "name": "generator-latex-template",
  "version": "0.1.0",
  "description": "Generates a latex template to be used as start for writing scientific works (papers, articles, theses)",
  "homepage": "https://github.com/latextemplates/generator-latex-template",
  "author": {
    "name": "Oliver Kopp",
    "email": "kopp.dev@gmail.com",
    "url": "http://orcid.org/0000-0001-6962-4290"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "latex",
    "bachelor thesis",
    "master thesis",
    "template",
    "yeoman-generator"
  ],
  "devDependencies": {
    "yeoman-test": "^1.7.0",
    "yeoman-assert": "^3.1.0",
    "eslint": "^5.9.0",
    "prettier": "^1.15.2",
    "husky": "^1.2.0",
    "lint-staged": "^8.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-config-xo": "^0.25.0",
    "jest": "^23.5.0"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "yeoman-generator": "^2.0.1",
    "chalk": "^2.1.0",
    "yosay": "^2.0.1"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "pretest": "eslint .",
    "test": "jest"
  },
  "repository": "https://github.com/latextemplates/generator-latex-template.git",
  "license": "GPL-3.0"
}
