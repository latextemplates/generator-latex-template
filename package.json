{
  "name": "generator-latex-template",
  "version": "2025.4.11",
  "description": "Generates a latex template to be used as start for writing scientific works (papers, articles, theses)",
  "homepage": "https://github.com/latextemplates/generator-latex-template",
  "author": {
    "name": "Oliver Kopp",
    "email": "kopp.dev@gmail.com",
    "url": "http://orcid.org/0000-0001-6962-4290"
  },
  "type": "module",
  "files": [
    "generators"
  ],
  "main": "generators/app/index.js",
  "keywords": [
    "latex",
    "bachelor thesis",
    "master thesis",
    "template",
    "yeoman-generator"
  ],
  "devDependencies": {
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-config-xo": "^0.47.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^9.0.6",
    "jest": "^29.6.1",
    "lint-staged": "^15.1.0",
    "prettier": "^3.0.0"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "chalk": "~4.1.2",
    "minimist": "^1.2.8",
    "patch-package": "^8.0.0",
    "yeoman-environment": "^4.0.0",
    "yeoman-generator": "~7.5.0",
    "yeoman-option-or-prompt": "^2.0.1",
    "yosay": "^3.0.0"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "pretest": "eslint .",
    "test": "jest",
    "postinstall": "patch-package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/latextemplates/generator-latex-template.git"
  },
  "license": "0BSD"
}
